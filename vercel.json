{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE","PATCH"]
    }
  ],
  "buildCommand": "yarn vercel-build",
  "devCommand": "yarn start:dev",
  "installCommand": "yarn",
  "functions": {
    "src/common/middleware/auth.middleware.ts": {
      "runtime": "node@18.x"
    }
  }

}
